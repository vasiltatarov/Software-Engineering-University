<body>
    <header>
        <!-- ======= Property Search Section ======= -->
        <div class="click-closed"></div>
        <!--/ Form Search Star /-->
        <div class="box-collapse">
            <div class="title-box-d">
                <h3 class="title-d">Search Property</h3>
            </div>
            <span class="close-box-collapse right-boxed bi bi-x"></span>
            <div class="box-collapse-wrap">
                <form class="form-a" method="get" asp-controller="Properties" asp-action="GetProperties">
                    <div class="row">
                        <div class="col-md-12 mb-2">
                            <label class="pb-2">Ключова дума <span>(квартал: дианабад, център...)</span></label>
                            <input type="text" class="form-control form-control-lg form-control-a" name="keyword" placeholder="Търсене по ключова дума">
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="mt-3">
                                <label class="pb-2">Цена от:</label>
                                <input class="form-control form-select form-control-a" name="minPrice" placeholder="Минимална цена">
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="mt-3">
                                <label class="pb-2">Цена до:</label>
                                <input class="form-control form-select form-control-a" name="maxPrice" placeholder="Максимална цена">
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="mt-3">
                                <label class="pb-2">Квадратура от:</label>
                                <input class="form-control form-select form-control-a" name="minSquareMeter" placeholder="Минимална Квадратура">
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="mt-3">
                                <label class="pb-2">Квадратура до:</label>
                                <input class="form-control form-select form-control-a" name="minSquareMeter" placeholder="Минимална Квадратура">
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="mt-3">
                                <label class="pb-2">Тип Жилище</label>
                                <select class="form-control" name="type">
                                    <option>Всички...</option>
                                    <option value="Къща">Къща</option>
                                    <option value="АТЕЛИЕ, ТАВАН">АТЕЛИЕ, ТАВАН</option>
                                    <option value="1-СТАЕН">1-Стаен</option>
                                    <option value="2-СТАЕН">2-Стаен</option>
                                    <option value="3-СТАЕН">3-Стаен</option>
                                    <option value="4-СТАЕН">4-Стаен</option>
                                    <option value="МЕЗОНЕТ">МЕЗОНЕТ</option>
                                    <option value="МНОГОСТАЕН">МНОГОСТАЕН</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="mt-3">
                                <label class="pb-2">Тип Строителство</label>
                                <select class="form-control" name="buildingType">
                                    <option>Всички...</option>
                                    <option value="Тухла">Тухла</option>
                                    <option value="Панел">Панел</option>
                                    <option value="Гредоред">Гредоред</option>
                                    <option value="ЕПК">ЕПК</option>
                                    <option value="ПК">ПК</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <button type="submit" asp-controller="Properties" asp-action="GetProperties" class="btn btn-b">Search Property</button>
                        </div>
                    </div>
                </form>
            </div>
        </div><!-- End Property Search Section -->>

        <!-- ======= Header/Navbar ======= -->
        <nav class="navbar navbar-default navbar-trans navbar-expand-lg fixed-top">
            <div class="container">
                <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarDefault" aria-controls="navbarDefault" aria-expanded="false" aria-label="Toggle navigation">
                    <span></span>
                    <span></span>
                    <span></span>
